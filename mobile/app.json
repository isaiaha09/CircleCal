{
  "expo": {
    "name": "CircleCal",
    "slug": "circlecal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "circlecal",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "app.circlecal.mobile",
      "buildNumber": "1",
      "icon": "./assets/appicon.png",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "CircleCal needs camera access to let you take a profile picture.",
        "NSPhotoLibraryUsageDescription": "CircleCal needs photo library access to let you choose and upload a profile picture.",
        "NSPhotoLibraryAddUsageDescription": "CircleCal needs photo library access to save or upload images you choose.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "app.circlecal.mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiBaseUrl": "https://circlecal.app",
      "eas": {
        "projectId": "395d51e7-e2d3-4775-9404-12e4ecb9bedc"
      }
    },
    "owner": "ia09"
  }
}
